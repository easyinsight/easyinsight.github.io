<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN" "http://java.sun.com/dtd/web-app_2_3.dtd">
<web-app>

    <display-name>eiapps</display-name>
    <description>eiapps</description>

    <servlet>
        <servlet-name>caesarsappend</servlet-name>
        <display-name>caesarsappend</display-name>
        <servlet-class>com.easyinsight.sync.CaesarsSync</servlet-class>
        <load-on-startup>0</load-on-startup>
    </servlet>

    <servlet>
        <servlet-name>rcfranklin-statuses</servlet-name>
        <display-name>rcfranklin-statuses</display-name>
        <servlet-class>com.easyinsight.sync.rescare.franklin.StatusSync</servlet-class>
        <load-on-startup>0</load-on-startup>
    </servlet>

    <servlet>
        <servlet-name>vose</servlet-name>
        <display-name>vose</display-name>
        <servlet-class>com.easyinsight.sync.VoseServlet</servlet-class>
        <load-on-startup>0</load-on-startup>
    </servlet>

    <servlet-mapping>
        <servlet-name>caesarsappend</servlet-name>
        <url-pattern>/caesarsappend</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>rcfranklin-statuses</servlet-name>
        <url-pattern>/rescare/franklin/statuses</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>vose</servlet-name>
        <url-pattern>/vose</url-pattern>
    </servlet-mapping>


</web-app>
